{
	"manifest_version": 3,
	"name": "My Window Alert Extension",
	"version": "1.0",
	"description": "Alert items from an array on new window open",
	"permissions": ["tabs"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"48": "logo.png"
	}
}
